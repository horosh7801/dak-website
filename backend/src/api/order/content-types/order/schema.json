{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "date": {
      "type": "datetime"
    },
    "item": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "accepted",
        "fulfilled"
      ],
      "default": "pending",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item"
    }
  }
}
